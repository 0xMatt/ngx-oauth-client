{
  "project": {
    "name": "ngx-oauth-client"
  },
  "apps": [
    {
      "root": "demo/src",
      "outDir": "demo/dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "test": "../../scripts/test.ts",
      "tsconfig": "tsconfig.app.json",
      "prefix": "",
      "styles": [
        "styles.css"
      ],
      "scripts": [
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "addons": [],
  "packages": [],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "demo/src/tsconfig.app.json"
    },
    {
      "project": "demo/src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    },
    "codeCoverage": {
      "exclude": [
        "scripts/**/*",
        "demo/**/*",
        "dist/**/*",
        "e2e/**/*",
        "tmp/**/*",
        "*.js"
      ]
    }
  },
  "defaults": {
    "styleExt": "css",
    "prefixInterfaces": false,
    "inline": {
      "style": false,
      "template": false
    },
    "spec": {
      "class": false,
      "component": true,
      "directive": true,
      "module": false,
      "pipe": true,
      "service": true
    }
  }
}
